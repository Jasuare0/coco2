<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- <title>E-mprender</title> -->
    
    <%- include('./partials/title') %>


        
    <!-- ESTILOS BOOSTRAP -->

    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">

    <!-- ESTILOS PERSONALES -->

    <meta name="viewport" content="width=device-width, initial-scale=1.0">


    <link rel='stylesheet' href='/stylesheets/index.css' />
    <link rel='stylesheet' href='/stylesheets/nosotros.css' />
    <link rel='stylesheet' href='/stylesheets/navbar.css' />


    <!-- FUENTES -->

    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="<%= fuentes[0].link %>" rel="stylesheet">



</head>

<body style="background-color: rgba(<%= resultados[14].value %>,<%= resultados[15].value %>,<%= resultados[16].value %>, <%= (resultados[17].value)/10 %>);<%= fuentes[0].css %>">

    
    <!-- INICIO BARRA DE NAVEGACIÓN -->

    <%- include('./partials/navbar') %>

    <!-- FIN BARRA DE NAVEGACIÓN -->


    <!-- INICIO CARROUSEL DE IMAGENES -->

    <%- include('./partials/carousel') %>

    <!-- FIN CARROUSEL DE IMAGENES -->

    

    <div class="external-container-section">

        <div class="internal-container-section">
            <div class="external-container-header-nosotros">
                <div class="internal-container-header-nosotros">
                    <h2 class="header-sobre-nosotros"><%= nosotros[0].item %></h2>
                </div>
                <div class="container-paragraph-nosotros">
                    <p class="paragraph-nosotros"><%= nosotros[0].value %></p>
                </div>
            </div>
        </div>
  
    </div>      



    <div class="external-container-section">

        <div class="internal-container-section">
          <h2 class="header-section"><%= nosotros[1].item %></h2>
          <p class="paragraph-objetivo"><%= nosotros[1].value %></p>
        </div>
  
    </div>

    <div class="external-container-section">

        <div class="internal-container-section">
            <div class="external-container-mision-vision">
                <div class="internal-container-mision-vision">
                    <h2 class="header-mision"><%= nosotros[2].item %></h2>
                    <p class="paragraph-mision"><%= nosotros[2].value %></p>
                </div>
                <div class="internal-container-mision-vision">
                    <h2 class="header-vision"><%= nosotros[3].item %></h2>
                    <p class="paragraph-vision"><%= nosotros[3].value %></p>
                </div>
            </div>

        </div>
  
    </div>

    

    

    <div class="external-container-section">

        <div class="internal-container-section">
          <h2 class="header-section">Nuestros Valores</h2>
        </div>
  
    </div>
      
    <div class="external-container-section">

        <div class="internal-container-section">
            <div class="external-container-valores">
                <div class="internal-container-valores">
                    <h2 class="header-valor"><%= nosotros[4].item %></h2>
                    <a href="" class="a-icono-valores"><i class="far fa-thumbs-up icons-valores"></i></a>
                    <p class="paragraph-valores"><%= nosotros[4].value %></p>
                </div>

                <div class="internal-container-valores">
                    <h2 class="header-valor"><%= nosotros[5].item %></h2>
                    <a href="" class="a-icono-valores"><i class="fas fa-check-double icons-valores"></i></a>
                    <p class="paragraph-valores"><%= nosotros[5].value %></p>
                </div>

                <div class="internal-container-valores">
                    <h2 class="header-valor"><%= nosotros[6].item %></h2>
                    <a href="" class="a-icono-valores"><i class="fas fa-chart-line icons-valores"></i></a>
                    <p class="paragraph-valores"><%= nosotros[6].value %></p>
                </div>

            </div>
        </div>
  
        
    </div>


    <!-- INICIO FOOTER -->

    <%- include('./partials/footer') %>

    <!-- FIN FOOTER -->


</body>


  <!-- ESTILOS BOOSTRAP -->
  <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js" integrity="sha384-9/reFTGAW83EW2RDu2S0VKaIzap3H66lZH81PoYlFhbGU+6BZp6G7niu735Sk7lN" crossorigin="anonymous"></script>
  <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js" integrity="sha384-B4gt1jrGC7Jh4AgTPSdUtOBvfO8shuf57BaghqFfPlYxofvL8/KUEfYiJOMMV+rV" crossorigin="anonymous"></script>

  <!-- iconos gratuitos -->
